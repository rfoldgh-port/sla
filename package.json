{
  "name": "ski-lift-app",
  "version": "1.0.0",
  "engines": {
    "node": "4.6.0"
  },
  "description": "A React template",
  "main": "js/index.js",
  "scripts": {
    "start": "supervisor server.js",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\"",
    "mkdir:images": "run-s mkdir",
    "clean:images": "rimraf build/images",
    "build:images": "run-s clean:images mkdir:images _build:images",
    "_build:images": "cpr images build/images",
    "watch:images": "run-s build:images _watch:images",
    "_watch:images": "chokidar images/**/* -c \"npm run build:images\""
  },
  "author": "Joe Turner <joe@oampo.co.uk>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "mongoose": "^5.9.23",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.5.4",
    "run-p": "0.0.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "supervisor": "^0.12.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.16.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "dotenv-safe": "^4.0.4",
    "express": "^4.14.0",
    "express-session": "^1.15.1",
    "googleapis": "^16.1.0",
    "isomorphic-fetch": "^2.2.1",
    "json-parser": "^1.1.5",
    "material-ui": "^0.17.0",
    "mongodb": "^2.2.27",
    "mongoose": "^5.9.23",
    "nonce": "^1.0.4",
    "oauth-signature": "^1.3.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-google-maps": "^6.0.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "session": "^0.1.0",
    "supervisor": "^0.12.0",
    "yelp": "^1.0.2"
  }
}
